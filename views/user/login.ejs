<!DOCTYPE html>
<html lang="en">

<%- include('../partials/head') %>

<body>
    <%- include('../partials/header') %>

    <main>
        <section class="register">
            <div class="form-title">
                <h1>Ingresá a tu cuenta</h1>
            </div>
            <form action="/" method="get">
                <div>
                    <input class="wide-input" placeholder="Usuario" type="text" name="usuario" required>
                </div>
                <div>
                    <input class="wide-input" placeholder="Contraseña" type="password" name="contraseña" required>
                </div>
                <div class="button-div">
                    <input class="button-input" type="submit" value="Ingresar">
                </div>
            </form>
        </section>
    <%- include('../partials/footer') %>

    </main>
</body>